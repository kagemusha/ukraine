<Countdown />

<div class="main-timeline">

    {{#each this.model as |event|}}
        <TimelineEvent @event={{event}} />
    {{/each}}

    <div class="victory-event">
        <div class="victory-date">
            <span class="event-date-day">Day 182</span>
            <span class="victory-date-text">Independence Day 2022</span>
            <span class="event-date-date">2022-08-24</span>
        </div>

        <div class="event-text">
                The orcs will be scurrying back to the Misty Mountains and the era of a
                free Ukraine will be dawning
        </div>
        <div class="event-image">
            <Image @src='independence_momument_XoczqK_Dq.png' />
        </div>
    </div>
</div>
